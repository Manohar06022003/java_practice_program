package practice_session;
import java .util. Scanner;
public class practice_2 {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int count=0;
		
		while(sc.hasNext()) {
			
			String m=sc.nextLine();
			count+=1;
			
			System.out.println(count+" "+m);
		}
		

	}

}
