package classobjects;

public class phone {
	
		String name="";
		int price=0;
		int ram =0;
		String proc="";
		String model="";

	
	public static void main(String args[]) {
		phone seller1=new phone();
		seller1.name="iphone";
		seller1.price=23;
		seller1.ram=4;
		seller1.proc="dssff";
		seller1.model="tdydg";
		System.out.println(seller1.name);		
	}
}
